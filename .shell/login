# Put your environment variables here

# Preferred Programs
export PAGER=less
export EDITOR=vim
export VISUAL=$EDITOR

# Path Settings
pathprepend ~/bin PATH
pathprepend /usr/local/bin PATH
pathprepend /usr/bin PATH
pathprepend /bin PATH
pathprepend /usr/sbin/ PATH
pathprepend /sbin PATH
pathprepend /usr/local/heroku/bin PATH
